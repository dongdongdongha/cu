	FOR I=1 TO N
		MCIR P8, P2
	ENDF
ENDI
IF (GPNT(512).1 < (P5.1 - 100))
	MPTP P4
	FOR I=1 TO N
		MCIR P2, P8
	ENDF
ENDI
MPTP P5
GOTO A1
MEND

